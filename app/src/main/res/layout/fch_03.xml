		<LinearLayout 
            android:layout_weight="1" 
            android:layout_width="0dip"
		    android:layout_height="match_parent"
			android:orientation="horizontal" 
		    >
			<ImageView android:id="@+id/asOut"
	            android:layout_width="30dp"
	            android:layout_height="30dp"
	            android:layout_marginLeft="4dp"
	            android:layout_gravity="center_vertical|center_horizontal"
	            android:background="@drawable/as_out" 
	            android:focusable="false" 
	            android:contentDescription="@string/image_description"/>    
	    </LinearLayout>		
		
		<LinearLayout 
            android:layout_weight="1" 
            android:layout_width="0dip"
		    android:layout_height="match_parent"
			android:orientation="horizontal" 
		    >
			<ImageView android:id="@+id/asIn"
	            android:layout_width="30dp"
	            android:layout_height="30dp"
	            android:layout_marginLeft="4dp"
	            android:layout_gravity="center_vertical|center_horizontal"
	            android:background="@drawable/as_in" 
	            android:focusable="false" 
	            android:contentDescription="@string/image_description"/>    
	    </LinearLayout>		
		-->		
		<LinearLayout 
            android:layout_weight="1" 
            android:layout_width="0dip"
		    android:layout_height="match_parent"
			android:orientation="horizontal" 
		    >
			<ImageView android:id="@+id/asStart"
	            android:layout_width="30dp"
	            android:layout_height="30dp"
	            android:layout_marginLeft="4dp"
	            android:layout_gravity="center_vertical|center_horizontal"
	            android:background="@drawable/aq_start" 
	            android:focusable="false" 
	            android:contentDescription="@string/image_description"/>    
	    </LinearLayout>		
					
		<LinearLayout 
            android:layout_weight="1" 
            android:layout_width="0dip"
		    android:layout_height="match_parent"
			android:orientation="horizontal" 
		    >
			<ImageView android:id="@+id/asPause"
	            android:layout_width="30dp"
	            android:layout_height="30dp"
	            android:layout_marginLeft="4dp"
	            android:layout_gravity="center_vertical|center_horizontal"
	            android:background="@drawable/aq_pause" 
	            android:focusable="false" 
	            android:contentDescription="@string/image_description"/>    
	    </LinearLayout>		
			
		<LinearLayout 
            android:layout_weight="1" 
            android:layout_width="0dip"
		    android:layout_height="match_parent"
			android:orientation="horizontal" 
		    >
			<ImageView android:id="@+id/asResume"
	            android:layout_width="30dp"
	            android:layout_height="30dp"
	            android:layout_marginLeft="4dp"
	            android:layout_gravity="center_vertical|center_horizontal"
	            android:background="@drawable/aq_resume" 
	            android:focusable="false" 
	            android:contentDescription="@string/image_description"/>    
	    </LinearLayout>		
			
		<LinearLayout 
            android:layout_weight="1" 
            android:layout_width="0dip"
		    android:layout_height="match_parent"
			android:orientation="horizontal" 
		    >
			<ImageView android:id="@+id/asStop"
	            android:layout_width="30dp"
	            android:layout_height="30dp"
	            android:layout_marginLeft="4dp"
	            android:layout_gravity="center_vertical|center_horizontal"
	            android:background="@drawable/aq_stop" 
	            android:focusable="false" 
	            android:contentDescription="@string/image_description"/>    
	    </LinearLayout>		

		<TextView android:id="@+id/asStatus"
		    android:layout_width="50dp" 
		    android:layout_height="match_parent" 
		    android:gravity="center_vertical"
		    android:text="@string/txtStop" 
	        android:textSize="@dimen/text14" />
						
	</LinearLayout>
